{
  "name": "common-styles",
  "version": "1.0.0",
  "description": "Proyecto que usa Bun para compilar Sass a CSS",
  "main": "src/index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "sass src/scss/main.scss dist/main.css --style=compressed",
    "build:ts": "bun run build.ts",
    "dev": "bun run dev"
  },
  "dependencies": {
    "@unocss/reset": "^66.1.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "sass": "^1.89.0"
  },
  "peerDependencies": {},
  "exports": {
    ".": "./dist/main.css",
    "./scss": "./src/scss",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/"
  ],
  "bun": {
    "version": "latest"
  },
  "author": "Ivan Falcon",
  "license": "MIT"
}
